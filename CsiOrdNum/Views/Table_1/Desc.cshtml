@model IEnumerable<CsiOrdNum.Models.Order02>
@{
    ViewBag.Title = "Desc";
}


<h4>Descending Order</h4>

<hr>

<table class="table table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EntryValue)
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(model => item.EntryValue)
            </td>
        </tr>

    }

</table>

<p> @Html.ActionLink("Enter Value", "Create")    </p>
<p> @Html.ActionLink("Ascending Order", "Asc")   </p>
<p> @Html.ActionLink("Database Entries", "Index")    </p>
